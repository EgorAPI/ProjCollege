        </Grid.Background>

        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <StackPanel Orientation="Vertical">
            <TextBlock FontSize="24" Margin="10" Foreground="White"/>
        </StackPanel>

    </Grid>
</Page>
